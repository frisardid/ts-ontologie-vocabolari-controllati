#################################################################
#    Classes#################################################################

###  https://w3id.org/italia/onto/ACO/AdministrativeDeed
aco:AdministrativeDeed rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedIssuedBy ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedRegisteredIn ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:depositedIn ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedNumber ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedTitle ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedYear ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ;
                       rdfs:comment "Atto giuridico posto in essere da un'autorità amministrativa nell'esercizio di una sua funzione amministrativa."@it ;
                       rdfs:label "Administrative deed"@en ,
                                  "Atto amministrativo"@it .


