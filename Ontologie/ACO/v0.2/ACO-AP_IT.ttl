clv:City rdfs:label "Comune"@it .


cpv:dateOfBirth rdfs:label "data di nascita"@it .


cpv:familyName rdfs:label "cognome"@it .


cpv:givenName rdfs:label "nome"@it .


#################################################################
#    General axioms#################################################################
[ owl:intersectionOf ( aco:Applicant
                       [ rdf:type owl:Restriction ;
                         owl:onProperty aco:roleRelativeToApplication ;
                         owl:someValuesFrom [ rdf:type owl:Class ;
                                              owl:unionOf ( aco:BirthCertificate
                                                            aco:ResidenceCertificate
                                                          )
                                            ]
                       ]
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty [ owl:inverseOf aco:playsRole
                                   ] ;
                    owl:someValuesFrom [ rdf:type owl:Class ;
                                         owl:unionOf ( aco:PublicAdministration
                                                       rpo:RegisteredResidentPerson
                                                     )
                                       ]
                  ]
] .


[ owl:intersectionOf ( aco:ApplicationSubject
                       [ rdf:type owl:Restriction ;
                         owl:onProperty aco:roleRelativeToApplication ;
                         owl:someValuesFrom [ rdf:type owl:Class ;
                                              owl:unionOf ( aco:BirthCertificate
                                                            aco:ResidenceCertificate
                                                          )
                                            ]
                       ]
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty [ owl:inverseOf aco:playsRole
                                   ] ;
                    owl:someValuesFrom rpo:RegisteredResidentPerson
                  ]
] .


[ rdf:type owl:Class ;
  owl:unionOf ( aco:Applicant
                aco:ApplicationSubject
              ) ;
  rdfs:subClassOf aco:RoleInApplication
] .


[ rdf:type owl:Class ;
  owl:unionOf ( aco:CivilUnionReedRegistration
                aco:MarriageDeedRegistration
                aco:RegistrationOfAnnulledOrTerminatedCivilUnion
                aco:RegistrationOfAnnulledOrTerminatedWedding
                aco:RegistryEntryForBirth
              ) ;
  rdfs:subClassOf aco:RegistryEntry
] .


[ rdf:type owl:Class ;
  owl:unionOf ( aco:EvidenceOfBirthCertificate
                aco:EvidenceOfResidenceCertificate
              ) ;
  rdfs:subClassOf aco:Evidence
] .


[ rdf:type owl:Class ;
  owl:unionOf ( aco:MaritalStatusCertificate
                aco:RegistrationCertificate
                aco:WeddingCertificate
              ) ;
  rdfs:subClassOf aco:Certificate
] .


[ rdf:type owl:Class ;
  owl:unionOf ( aco:RegistrationOfAnnulledMarriage
                aco:RegistrationOfConcordatMarriageWithCeasedCivilEffects
                aco:RegistrationOfDissolvedMarriage
              ) ;
  rdfs:subClassOf aco:RegistrationOfAnnulledOrTerminatedWedding
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( aco:AnnotationOfAnnulledOrTerminatedCivilUnion
                aco:AnnotationOfAnnullmentOrTerminationOfWedding
                aco:BirthDeed
                aco:CitizenshipDeed
                aco:CivilUnionDeed
                aco:CivilUnionDissolutionDeed
                aco:DeathDeed
                aco:MarriageDeed
                aco:MarriageDissolutionDeed
                aco:SeparationDeed
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( aco:AnnulledWedding
                aco:ConcordatMarriageWithCeasedCivilEffects
                aco:DissolvedMarriage
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( aco:BirthCertificate
                aco:CertificateOfFamilyStatus
                aco:CitizenshipCertificate
                aco:ResidenceCertificate
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( aco:CivilUnionReedRegistration
                aco:MarriageDeedRegistration
                aco:RegistrationOfAnnulledOrTerminatedCivilUnion
                aco:RegistrationOfAnnulledOrTerminatedWedding
                aco:RegistryEntryForBirth
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( aco:RegistrationOfAnnulledMarriage
                aco:RegistrationOfConcordatMarriageWithCeasedCivilEffects
                aco:RegistrationOfDissolvedMarriage
              )
] .
