    <!-- https://w3id.org/italia/onto/ACO/AnnotationOfAnnulledOrTerminatedCivilUnion -->

    <owl:Class rdf:about="https://w3id.org/italia/onto/ACO/AnnotationOfAnnulledOrTerminatedCivilUnion">
